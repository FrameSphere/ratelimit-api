{
  "info": {
    "name": "RateLimit API",
    "description": "Complete API collection for RateLimit API testing",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8787",
      "type": "string"
    },
    {
      "key": "token",
      "value": "",
      "type": "string"
    },
    {
      "key": "api_key",
      "value": "",
      "type": "string"
    },
    {
      "key": "api_key_id",
      "value": "",
      "type": "string"
    },
    {
      "key": "config_id",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/",
          "host": ["{{base_url}}"]
        }
      }
    },
    {
      "name": "Auth",
      "item": [
        {
          "name": "Register",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('token', jsonData.token);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"test@example.com\",\n  \"password\": \"test123456\",\n  \"name\": \"Test User\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/register",
              "host": ["{{base_url}}"],
              "path": ["auth", "register"]
            }
          }
        },
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('token', jsonData.token);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"test@example.com\",\n  \"password\": \"test123456\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/login",
              "host": ["{{base_url}}"],
              "path": ["auth", "login"]
            }
          }
        },
        {
          "name": "Get Profile",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/auth/profile",
              "host": ["{{base_url}}"],
              "path": ["auth", "profile"]
            }
          }
        }
      ]
    },
    {
      "name": "API Keys",
      "item": [
        {
          "name": "Create API Key",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('api_key', jsonData.apiKey.apiKey);",
                  "    pm.collectionVariables.set('api_key_id', jsonData.apiKey.id);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"keyName\": \"Production API\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/keys",
              "host": ["{{base_url}}"],
              "path": ["api", "keys"]
            }
          }
        },
        {
          "name": "List API Keys",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/keys",
              "host": ["{{base_url}}"],
              "path": ["api", "keys"]
            }
          }
        },
        {
          "name": "Delete API Key",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/keys/{{api_key_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "keys", "{{api_key_id}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Configurations",
      "item": [
        {
          "name": "Create Config",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('config_id', jsonData.config.id);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"apiKeyId\": {{api_key_id}},\n  \"name\": \"Main RateLimit\",\n  \"maxRequests\": 100,\n  \"windowSeconds\": 3600\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/configs",
              "host": ["{{base_url}}"],
              "path": ["api", "configs"]
            }
          }
        },
        {
          "name": "Get Configs",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/configs/{{api_key_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "configs", "{{api_key_id}}"]
            }
          }
        },
        {
          "name": "Update Config",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Updated Config\",\n  \"maxRequests\": 200,\n  \"windowSeconds\": 3600,\n  \"enabled\": true\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/configs/{{config_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "configs", "{{config_id}}"]
            }
          }
        },
        {
          "name": "Delete Config",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/configs/{{config_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "configs", "{{config_id}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Filters",
      "item": [
        {
          "name": "Create Filter",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"configId\": {{config_id}},\n  \"ruleType\": \"ip_blacklist\",\n  \"ruleValue\": \"192.168.1.100\",\n  \"action\": \"block\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/filters",
              "host": ["{{base_url}}"],
              "path": ["api", "filters"]
            }
          }
        },
        {
          "name": "Get Filters",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/filters/{{config_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "filters", "{{config_id}}"]
            }
          }
        }
      ]
    },
    {
      "name": "RateLimit Check",
      "item": [
        {
          "name": "Check Rate Limit (GET)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{api_key}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/check?endpoint=/api/users&method=GET",
              "host": ["{{base_url}}"],
              "path": ["check"],
              "query": [
                {
                  "key": "endpoint",
                  "value": "/api/users"
                },
                {
                  "key": "method",
                  "value": "GET"
                }
              ]
            }
          }
        },
        {
          "name": "Check Rate Limit (POST)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{api_key}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/check?endpoint=/api/users&method=POST",
              "host": ["{{base_url}}"],
              "path": ["check"],
              "query": [
                {
                  "key": "endpoint",
                  "value": "/api/users"
                },
                {
                  "key": "method",
                  "value": "POST"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Analytics",
      "item": [
        {
          "name": "Get Analytics (24h)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/analytics/{{api_key_id}}?range=24h",
              "host": ["{{base_url}}"],
              "path": ["api", "analytics", "{{api_key_id}}"],
              "query": [
                {
                  "key": "range",
                  "value": "24h"
                }
              ]
            }
          }
        },
        {
          "name": "Get Analytics (7d)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/analytics/{{api_key_id}}?range=7d",
              "host": ["{{base_url}}"],
              "path": ["api", "analytics", "{{api_key_id}}"],
              "query": [
                {
                  "key": "range",
                  "value": "7d"
                }
              ]
            }
          }
        },
        {
          "name": "Get Recent Logs",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/logs/{{api_key_id}}?limit=50",
              "host": ["{{base_url}}"],
              "path": ["api", "logs", "{{api_key_id}}"],
              "query": [
                {
                  "key": "limit",
                  "value": "50"
                }
              ]
            }
          }
        }
      ]
    }
  ]
}
